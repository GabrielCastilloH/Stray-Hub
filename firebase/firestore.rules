rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // All data access goes through the FastAPI backend using a service account
    // (admin SDK), which bypasses these rules entirely. Block all direct client
    // access so that no mobile client can read/write Firestore without going
    // through the backend.
    match /{document=**} {
      allow read, write: if false;
    }
  }
}

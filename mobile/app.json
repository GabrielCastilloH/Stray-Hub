{
  "expo": {
    "name": "Shrub",
    "slug": "stray-hub",
    "version": "1.0.0",
    "scheme": "shrub",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#124170"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.strayhub.shrub"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#124170"
      },
      "package": "com.strayhub.shrub"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Shrub needs camera access to identify stray animals."
        }
      ],
      "expo-asset",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
